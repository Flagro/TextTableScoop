#!/usr/bin/env python
# -*- mode: python -*-
# PYTHON_ARGCOMPLETE_OK

import sys
from texttablescoop.cli import get_args
from texttablescoop.parser import process


def main():
    args = get_args()
    try:
        output = process(**vars(args))
    except Exception as e:
        sys.stderr.write(e + '\n')
        sys.exit(1)
    else:
        print(output)


main()
